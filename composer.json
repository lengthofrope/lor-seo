{
    "name": "nextbuzz/buzz-seo",
    "description": "A WordPress SEO plugin",
    "keywords": ["WordPress", "SEO", "Plugin"],
    "homepage": "https://github.com/nextbuzz/buzz-seo",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Bas de Kort",
            "email": "bdekort@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.0",
        "phptal/phptal": "^1.3",
        "lengthofrope/create-jsonld-tiny": ">=0.10",
        "yahnis-elsts/plugin-update-checker": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "NextBuzz\\SEO\\": "lib"
        }
    },
    "scripts": {
    "post-install-cmd": [
      "php composer-cleanup.php"
    ],
    "post-update-cmd": [
      "php composer-cleanup.php"
    ]
  }
}
